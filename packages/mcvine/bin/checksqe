#!/usr/bin/env python
import warnings
warnings.simplefilter('ignore')
import mcvine
warnings.simplefilter('default')

def buildBase():
    from mcvine.applications.InstrumentBuilder import build
    components = ['source', 'monitor']
    return build(components)

base = buildBase()

class App(base):

    def _defaults(self):
        super(App, self)._defaults()
        self.inventory.source = 'sources/NeutronFromStorage'
        self.inventory.monitor = 'monitors/IQE_monitor'
        return

def main():
    app = App('checksqe')
    return app.run()
        
if __name__ == '__main__': main()


# $Id$
